package com.niltech.beans;

public class Emp1 {
	private int eno;
	private String ename;
	private String address;

	public Emp1(int eno, String ename, String address) {
		System.out.println("Emp1.3-param constructor");
		this.eno = eno;
		this.ename = ename;
		this.address = address;
	}
	
	public Emp1(int eno, String ename) {
		System.out.println("Emp1.2-param constructor");
		this.eno = eno;
		this.ename = ename;
	}
	
	public Emp1(int eno,  String address) {
		System.out.println("Emp1.3-param constructor");
		this.eno = eno;
		this.address = address;
	}
	
	
	

	@Override
	public String toString() {
		return "Emp1 [eno=" + eno + ", ename=" + ename + ", address=" + address + "]";
	}
	
	
	
	
	

}
